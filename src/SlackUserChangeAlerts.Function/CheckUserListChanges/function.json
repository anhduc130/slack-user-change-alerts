{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "previousUsersJson",
      "type": "blob",
      "path": "slack/userlist.json",
      "connection": "StorageConnection",
      "direction": "in"
    },
    {
      "name": "$return",
      "type": "blob",
      "path": "slack/userlist.json",
      "connection": "StorageConnection",
      "direction": "out"
    }
  ]
}